{
  "name": "geyser-frontend",
  "version": "0.0.0",
  "author": "Julien Hauseux <julien.hauseux@univ-lille.fr>",
  "license": "AGPL-3.0-only",
  "private": true,
  "type": "module",
  "engines": {
    "node": "=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prettier": "prettier . --check",
    "eslint": "eslint --flag unstable_ts_config .",
    "typecheck": "tsc --noEmit -p tsconfig.node.json && vue-tsc --noEmit -p tsconfig.app.json",
    "validate": "pnpm run prettier && pnpm run eslint && pnpm run typecheck",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.15",
    "@urql/vue": "^1.4.2",
    "dotenv": "^16.4.7",
    "graphql": "^16.9.0",
    "keycloak-js": "^26.0.7",
    "quasar": "^2.17.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/client-preset": "^4.5.1",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@quasar/vite-plugin": "^1.8.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@urql/devtools": "^2.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sass": "^1.82.0",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  }
}
